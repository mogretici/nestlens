{
  "name": "nestlens",
  "version": "0.1.0",
  "description": "Laravel Telescope-like debugging and monitoring tool for NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc && npm run build:dashboard",
    "build:lib": "tsc",
    "build:dashboard": "cd dashboard && npm run build && mkdir -p ../dist/dashboard/public && cp -r dist/* ../dist/dashboard/public/",
    "dev": "tsc --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "debugging",
    "monitoring",
    "telescope",
    "devtools",
    "logging",
    "tracing",
    "profiling",
    "dashboard",
    "queries",
    "requests",
    "exceptions",
    "jobs",
    "cache",
    "redis",
    "bull",
    "prisma",
    "typeorm"
  ],
  "author": "mogretici",
  "license": "MIT",
  "homepage": "https://github.com/mogretici/nestlens#readme",
  "bugs": {
    "url": "https://github.com/mogretici/nestlens/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mogretici/nestlens.git"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^11.6.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@playwright/test": "^1.57.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
