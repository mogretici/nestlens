{
  "name": "nestlens",
  "version": "0.3.3",
  "description": "Laravel Telescope-like debugging and monitoring tool for NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rimraf dist && tsc && tsc-alias && npm run build:dashboard",
    "build:lib": "tsc && tsc-alias",
    "build:dashboard": "cd dashboard && npm run build && mkdir -p ../dist/dashboard/public && cp -r dist/* ../dist/dashboard/public/",
    "dev": "tsc --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:headed": "npx playwright test --headed",
    "prepublishOnly": "npm run build",
    "sync-versions": "node -e \"const fs=require('fs');const pkg=require('./package.json');const dash=require('./dashboard/package.json');dash.version=pkg.version;fs.writeFileSync('./dashboard/package.json',JSON.stringify(dash,null,2)+'\\n')\"",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "publish:patch": "npm run release:patch && git push --follow-tags && npm publish",
    "publish:minor": "npm run release:minor && git push --follow-tags && npm publish",
    "publish:major": "npm run release:major && git push --follow-tags && npm publish",
    "prepare": "husky"
  },
  "keywords": [
    "nestjs",
    "debugging",
    "monitoring",
    "telescope",
    "devtools",
    "logging",
    "tracing",
    "profiling",
    "dashboard",
    "queries",
    "requests",
    "exceptions",
    "jobs",
    "cache",
    "redis",
    "bull",
    "prisma",
    "typeorm",
    "graphql",
    "apollo",
    "mercurius",
    "subscriptions",
    "n+1"
  ],
  "author": "mogretici",
  "license": "MIT",
  "homepage": "https://github.com/mogretici/nestlens#readme",
  "bugs": {
    "url": "https://github.com/mogretici/nestlens/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mogretici/nestlens.git"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "better-sqlite3": "^11.0.0",
    "ioredis": "^5.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "@nestjs/apollo": {
      "optional": true
    },
    "@nestjs/mercurius": {
      "optional": true
    },
    "@apollo/server": {
      "optional": true
    },
    "mercurius": {
      "optional": true
    },
    "@nestjs/graphql": {
      "optional": true
    }
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "uuid": "^11.0.3"
  },
  "optionalDependencies": {
    "better-sqlite3": "^11.6.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/testing": "^11.0.0",
    "@playwright/test": "^1.57.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "semantic-release": "^25.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.6",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
